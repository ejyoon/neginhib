@book{bloom1970,
	Address = {New York, NY},
	Annote = {Conducted a longitudinal study of three children, Kathryn (21-24 months), Eric(19-27 months), and Gia(19-27 months).  Found many instances of external negation in these children's speech, and interpreted these utterances as anaphoric, i.e. \cite{bloom1970, bloom1993}: \begin{center}[The child cannot connect two toy trains]\\No [I can't/don't want to,] Lois do it. \end{center}  
In addition, Bloom catalogued the types of negative utterances that the three children in her study produced, identifying three basic types that developed in the same order in each child.  The first type of negation to emerge was nonexistence, in which negation is used the refer to an object that is not perceptually present.  The second type, which emerged shortly after nonexistence, was rejection, in which negation is used to refuse or turn away some object that is present in the context.  The final type of negation to emerge in Bloom's data was denial, in which the child forms a negative utterance to make a statement about something that is false in the world around them.  Bloom's analysis was one of the first to classify children's negations into different categories in this way.  \cite{bloom1970, bloom1993}},
	Author = {Bloom, L.},
	Date-Added = {2016-01-15 22:25:27 +0000},
	Date-Modified = {2016-01-15 22:25:27 +0000},
	Keywords = {negation},
	Publisher = {MIT Research Monograph, No 59.},
	Read = {0},
	School = {Columbia University},
	Title = {Language development: Form and function in emerging grammars.},
	Year = {1970}}

@incollection{pea1980,
	Address = {New York, NY},
	Annote = {Conducted an analysis of the types of negation seen in children's early speech.  Pea's study examined 6 children from 8 to 21 months, and examined both gestural and linguistic negations produced by these children over the course of a year.  Pea identified five different categories.  Rejection was the first to emerge in Pea's sample (gestures: 10-14 months, utterances: 12-15 months).  Disappearance (negations referring to an object that has recently disappeared or an action that has recently ceased), emerged consecutively with spoken rejection (utterances: 13-15 months), followed by self-prohibition (negations used in situations where the child is engaging in an action or interacting with an object that has been forbidden in the past; gestures: 11-16 months, utterances: 14-21 months), and unfulfilled expectation (negations used in cases where an object is not in its expected location or an expected action cannot occur; gestures: 14 months, utterances: 16-18 months).  The last to develop was Truth-Functional negation, or negations used by the child in situations in which the child is engaging in an action or interacting with an object that has been forbidden in the past (utterances: 19-23 months). },
	Author = {Pea, R.},
	Booktitle = {The Social Foundations of Language and Thought},
	Date-Added = {2016-01-15 22:25:16 +0000},
