@article{pea1982,
	annote = {Conducted a study which examined 2 -- 3-year-old children's spontaneous denials.  Children were shown either an object with a specific label or description, or a doll performing an action.  The experimenters then said sentences (for example,``that's the ball'', ``she's sitting'', or ``that's the yellow one'', or negative versions of similar sentences) that were either congruous with the scene or incongruous with the scene.  Results showed that while 30 -- 36 month olds responded to false affirmatives with a denial ``no'' significantly more than they did with true affirmatives, only some 24-month-olds produced ``no'' responses to false affirmatives, and 18-month-olds made almost no responses at all.  This study suggested that the spontaneous production of denial negations emerges somewhere around 2 years of age.},
	author = {Pea, R.D.},
	date-added = {2016-01-29 21:56:10 +0000},
	date-modified = {2016-01-29 21:56:10 +0000},
	journal = {Journal of Child Language},
	keywords = {negation},
	pages = {597-626},
	publisher = {Cambridge Univ Press},
	title = {Origins of verbal logic: Spontaneous denials by two-and three-year olds},
	volume = {9},
	year = {1982},
	bdsk-file-1 = {}}

